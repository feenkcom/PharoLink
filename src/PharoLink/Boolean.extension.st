Extension { #name : #Boolean }

{ #category : #'*PharoLink' }
Boolean >> isBoolean [
	"Answer a boolean indicating whether the receiver is a boolean"

	^ true
]

{ #category : #'*PharoLink' }
Boolean >> isPharoLinkPrimitive [
	"Answer a boolean indicating whether the receiver is a PharoLink primitive object"

	^ true
]
